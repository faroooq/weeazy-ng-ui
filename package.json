{
  "name": "weeazy-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint weeazy-app --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\"",
    "dev:ssr": "ng run weeazy-app:serve-ssr",
    "serve:ssr": "node dist/weeazy-app/server/main.js",
    "build:ssr": "ng build && ng run weeazy-app:server",
    "prerender": "ng run weeazy-app:prerender"
  },
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.1",
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/forms": "^14.0.4",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/platform-server": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@angular/service-worker": "^14.0.4",
    "@nebular/date-fns": "^9.0.3",
    "@nebular/eva-icons": "9.0.3",
    "@nebular/moment": "^9.0.3",
    "@nebular/theme": "9.0.3",
    "@nguniversal/express-engine": "^14.0.2",
    "@types/google.accounts": "0.0.2",
    "bootstrap": "5.1.3",
    "chart.js": "3.8.0",
    "classlist": "^2.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "^3.23.3",
    "date-fns": "^2.28.0",
    "echarts": "^5.3.3",
    "eva-icons": "^1.1.3",
    "express": "^4.18.1",
    "intl": "1.2.5",
    "moment": "^2.29.3",
    "nebular-icons": "1.1.0",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.7.2",
    "ngx-echarts": "^14.0.0",
    "ngx-quill": "^17.1.1",
    "ngx-smooth-dnd": "^0.4.1",
    "normalize.css": "8.0.1",
    "pace-js": "1.2.4",
    "quill": "^1.3.7",
    "quill-blot-formatter": "^1.0.5",
    "quill-emoji": "^0.2.0",
    "quill-image-compress": "^1.2.26",
    "quill-mention": "^3.1.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "7.5.5",
    "socicon": "3.0.5",
    "style-loader": "^3.3.1",
    "time-ago-pipe": "^1.3.2",
    "tsickle": "^0.46.0",
    "tslib": "^2.4.0",
    "typeface-exo": "1.1.13",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.6"
